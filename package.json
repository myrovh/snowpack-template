    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "prettier": "^1.19.1",
    "prettier-plugin-package": "^0.3.1",
    "servor": "^3.2.0",
    "snowpack": "^1.2.0"
  }
}
